package com.mantarTeknoloji.mutercim.entities;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;

@Entity
@Table(name = "product")
public class Verb {
	@Column(name="name")
	private String tr;

	public Verb() {}
	
	public Verb(String tr) {
		super();
		this.tr = tr;
	}

	public String getTr() {
		return tr;
	}

	public void setTr(String tr) {
		this.tr = tr;
	}
	
	
}
