package com.mantarTeknoloji.mutercim.Entities;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;

@Entity
@Table(name="verb")
public class Verb {
	@Column(name="tr")
	private String tr;

	@Column(name="osm")
	private String osm;
	
	public Verb() {}

	public Verb(String tr, String osm) {
		super();
		this.tr = tr;
		this.osm = osm;
	}

	public String getTr() {
		return tr;
	}

	public void setTr(String tr) {
		this.tr = tr;
	}

	public String getOsm() {
		return osm;
	}

	public void setOsm(String osm) {
		this.osm = osm;
	}
	
	
}
