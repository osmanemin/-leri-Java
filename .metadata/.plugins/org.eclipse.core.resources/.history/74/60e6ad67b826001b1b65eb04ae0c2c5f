package com.mantarTeknoloji.mutercim.entities;


import javax.persistence.*;

@Entity
@Table(name="verb")
public class Verb {
		
	@Column(name = "Name")
	private String name;
	
	
	public Verb() {}
	
	public Verb(String name) {
		super();
		this.name = name;
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
}
